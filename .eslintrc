{
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "project": "./tsconfig.json",
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "implicit-arrow-linebreak": "off",
    "import/prefer-default-export": "off",
    "max-len": ["error", { "code": 120 }],
    "no-console": "off",
    "no-continue": "off",
    "no-restricted-syntax": "off"
  }
}
